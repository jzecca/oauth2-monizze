{
    "name": "jzecca/oauth2-monizze",
    "description": "Monizze OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jérôme Zecca",
            "email": "jerome.zecca@cpiglobal.com"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication",
        "monizze"
    ],
    "require": {
        "php": ">=7.3",
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "eloquent/phony-phpunit": "^7.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Jzecca\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jzecca\\OAuth2\\Client\\Test\\": "tests/"
        }
    },
    "scripts": {
        "fix": "vendor/bin/php-cs-fixer fix",
        "test": "vendor/bin/phpunit"
    }
}
